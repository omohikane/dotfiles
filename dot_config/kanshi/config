# Dual EV2795
profile home-dual-ev2795 {
  output eDP-1 {
    mode 1920x1200
    position 0,1440
    status enable
  }
  output "Eizo Nanao Corporation EV2795 52324062" {
    mode 2560x1440
    position 0,0
    status enable
  }
  output "Eizo Nanao Corporation EV2795 52326062" {
    mode 2560x1440
    position 2560,0
    status enable
  }
  output "*" {
    status disable
  }
}

